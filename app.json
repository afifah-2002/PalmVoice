{
  "expo": {
    "name": "PalmVoice",
    "slug": "PalmVoice",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "palmvoice",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCalendarsUsageDescription": "PalmVoice needs access to your calendar to create events from tasks.",
        "NSRemindersUsageDescription": "PalmVoice needs access to reminders to create reminders from tasks.",
        "LSApplicationQueriesSchemes": [
          "googlegmail",
          "ms-outlook",
          "whatsapp",
          "trello",
          "googledrive",
          "zoomus",
          "msteams",
          "comgooglemeet",
          "canvas-student",
          "x-apple-health",
          "paypal",
          "com.amazon.mobile.shopping",
          "uber",
          "calshow",
          "x-apple-reminderkit"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
